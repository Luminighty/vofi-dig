{
	"compilerOptions": {
			"outDir": "./public_html/",
			"sourceMap": true,
			"strict": true,
			"noImplicitReturns": true,
			"noImplicitAny": false,
			"module": "es6",
			"moduleResolution": "node",
			"target": "es5",
			"allowJs": true,
			"allowSyntheticDefaultImports": true,
	},
	"include": [
			"./src/**/*"
	]
}